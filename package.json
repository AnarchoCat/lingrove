		"@vitejs/plugin-vue": "^5.2.1",
		"@vscode/test-cli": "^0.0.10",
		"@vscode/test-electron": "^2.4.1",
		"eslint": "^9.19.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-vue": "^9.32.0",
		"globals": "^15.14.0",
		"husky": "^9.1.7",
		"postcss": "^8.5.1",
		"prettier": "^3.4.2",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.22.0",
