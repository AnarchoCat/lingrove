{
	"name": "mmimy",
	"displayName": "Maomao i Moyu",
	"version": "0.0.1",
	"main": "dist/extension.js",
	"engines": {
		"node": "^23.6.0",
		"vscode": "^1.96.0"
	},
	"publisher": "AnarchoCat",
	"activationEvents": [],
	"contributes": {
		"configuration": {
			"title": "Mmimy Settings",
			"properties": {
				"mmimy.defaultLanguage": {
					"type": "string",
					"default": "en",
					"enum": [
						"en",
						"vi",
						"es",
						"pt",
						"fr"
					],
					"enumItemLabels": [
						"English",
						"Tiáº¿ng Viá»‡t",
						"EspaÃ±ol",
						"PortuguÃªs",
						"FranÃ§ais"
					],
					"enumDescriptions": [
						"ðŸ‡ºðŸ‡¸ðŸ‡¬ðŸ‡§",
						"ðŸ‡»ðŸ‡³",
						"ðŸ‡ªðŸ‡¸",
						"ðŸ‡µðŸ‡¹ðŸ‡§ðŸ‡·",
						"ðŸ‡«ðŸ‡·"
					],
					"description": "Select a language."
				},
				"mmimy.dictionaryFileName": {
					"type": "string",
					"default": "dictionary.json",
					"description": "The file name (.json) of the dictionary."
				}
			}
		},
		"commands": [
			{
				"command": "mmimy.selectLanguage",
				"title": "Select language"
			},
			{
				"command": "mmimy.saveDictionary",
				"title": "Save dictionary"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"icon": "dist/iconDictionary.svg",
					"id": "dictionary",
					"title": "Dictionary"
				}
			]
		},
		"views": {
			"dictionary": [
				{
					"type": "webview",
					"id": "mmimy.dictionaryView",
					"title": "Dictionary View",
					"name": "Dictionary"
				},
				{
					"type": "webview",
					"id": "mmimy.searchView",
					"title": "Search View",
					"name": "Search"
				}
			]
		}
	},
	"scripts": {
		"dev": "vite build --mode development",
		"dev:watch": "vite build --mode development --watch",
		"build": "vite build",
		"build:watch": "vite build --watch",
		"build-media": "vite build --config vite.config.media.ts",
		"build-media:watch": "vite build --config vite.config.media.ts --watch",
		"check": "tsc --noEmit",
		"package": "vsce package -o dist",
		"format": "prettier . --write",
		"lint": "eslint .",
		"prepare": "husky",
		"commitlint": "commitlint --edit",
		"pretest": "npm run dev",
		"test": "NODE_TLS_REJECT_UNAUTHORIZED=0 vscode-test"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/AnarchoCat/mmimy.git"
	},
	"keywords": [],
	"author": "Anarcho Cat <d2717p@gmail.com>",
	"license": "GPL-3.0-only",
	"bugs": {
